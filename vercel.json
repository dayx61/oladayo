{
  "framework": null,
  "buildCommand": "npm run build --workspace=client",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}
